{
    "pipeline": {
        "input": [
            "img"
        ],
        "output": [
            "post_output"
        ],
        "tasks": [
            {
                "type": "Task",
                "module": "Transform",
                "name": "Preprocess",
                "input": [
                    "img"
                ],
                "output": [
                    "prep_output"
                ],
                "transforms": [
                    {
                        "type": "LoadImageFromFile",
                        "backend_args": null
                    },
                    {
                        "type": "Resize",
                        "keep_ratio": true,
                        "size": [
                            640,
                            640
                        ]
                    },
                    {
                        "type": "Pad",
                        "size": [
                            640,
                            640
                        ],
                        "pad_val": {
                            "img": [
                                114,
                                114,
                                114
                            ]
                        }
                    },
                    {
                        "type": "Normalize",
                        "to_rgb": false,
                        "mean": [
                            103.53,
                            116.28,
                            123.675
                        ],
                        "std": [
                            57.375,
                            57.12,
                            58.395
                        ]
                    },
                    {
                        "type": "Pad",
                        "size_divisor": 1
                    },
                    {
                        "type": "DefaultFormatBundle"
                    },
                    {
                        "type": "Collect",
                        "meta_keys": [
                            "valid_ratio",
                            "img_norm_cfg",
                            "ori_shape",
                            "flip",
                            "flip_direction",
                            "scale_factor",
                            "pad_param",
                            "img_id",
                            "pad_shape",
                            "img_path",
                            "ori_filename",
                            "filename",
                            "img_shape"
                        ],
                        "keys": [
                            "img"
                        ]
                    }
                ]
            },
            {
                "name": "rtmdet",
                "type": "Task",
                "module": "Net",
                "is_batched": false,
                "input": [
                    "prep_output"
                ],
                "output": [
                    "infer_output"
                ],
                "input_map": {
                    "img": "input"
                },
                "output_map": {}
            },
            {
                "type": "Task",
                "module": "mmdet",
                "name": "postprocess",
                "component": "ResizeInstanceMask",
                "params": {
                    "nms_pre": 1000,
                    "min_bbox_size": 0,
                    "score_thr": 0.05,
                    "nms": {
                        "type": "nms",
                        "iou_threshold": 0.6
                    },
                    "max_per_img": 100,
                    "mask_thr_binary": 0.5,
                    "is_resize_mask": true
                },
                "output": [
                    "post_output"
                ],
                "input": [
                    "prep_output",
                    "infer_output"
                ]
            }
        ]
    }
}